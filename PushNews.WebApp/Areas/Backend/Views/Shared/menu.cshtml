@using Microsoft.AspNet.Identity;
@using PushNews.WebApp.Helpers;
@using UI = PushNews.WebApp.Models.UI;

@model IEnumerable<UI.ElementoMenu>

<div class="main-navigation navbar-collapse collapse">
    <!-- start: MAIN MENU TOGGLER BUTTON -->
    <div class="navigation-toggler">
        <i class="clip-chevron-left"></i>
        <i class="clip-chevron-right"></i>
    </div>
    <!-- end: MAIN MENU TOGGLER BUTTON -->
    <!-- start: MAIN NAVIGATION MENU -->
    @Html.MenuPrincipal(UI.Menu.GenerarMenu(User.Identity.GetUserId<long>(), (PushNews.Dominio.Entidades.Aplicacion) Session["Aplicacion"]),
                        new { @class = "main-navigation-menu", id = "mainMenu" })
    <!-- end: MAIN NAVIGATION MENU -->
</div>